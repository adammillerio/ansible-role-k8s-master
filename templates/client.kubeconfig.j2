apiVersion: v1
kind: Config
preferences: {}
current-context: service-account-context
contexts:
- context:
    cluster: local
    user: kubelet
  name: service-account-context
clusters:
- cluster:
    server: http://localhost:8080
  name: local
users:
- name: kubelet
  user:
    as-user-extra: {}
    client-certificate: /var/lib/kubelet/client.pem
    client-key: /var/lib/kubelet/client-key.pem
